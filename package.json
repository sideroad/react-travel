{
  "name": "react-travel",
  "version": "0.0.1",
  "description": "React Travel",
  "scripts": {
    "watch": "watchify app/browser.js -o dist/bundle.js -v",
    "css": "lessc css/app.css > dist/bundle.css",
    "wireframe": "cp wireframe.html dist/. && npm run css && cd dist && /usr/bin/python -m SimpleHTTPServer",
    "build": "browserify app/browser.js > dist/bundle.js && cp css/*.png dist/. && npm run css",
    "start": "babel-node app/server",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/sideroad/react-travel"
  },
  "keywords": [
    "react"
  ],
  "author": "sideroad",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/sideroad/react-travel/issues"
  },
  "homepage": "https://github.com/sideroad/react-travel",
  "devDependencies": {
    "babel-jest": "^4.0.0",
    "babelify": "^5.0.3",
    "browserify": "^9.0.3",
    "grunt": "^0.4.5",
    "grunt-browserify": "^3.7.0",
    "grunt-concurrent": "^1.0.0",
    "grunt-contrib-copy": "^0.8.0",
    "grunt-contrib-less": "^1.0.1",
    "grunt-contrib-watch": "^0.6.1",
    "grunt-nodemon": "^0.4.0",
    "grunt-spawn": "^1.1.72",
    "grunt-watchify": "^0.1.0",
    "jest-cli": "^0.4.0",
    "load-grunt-tasks": "^3.1.0",
    "superagent-mock": "^1.0.0",
    "watchify": "^2.2.1"
  },
  "dependencies": {
    "babel": "^4.5.5",
    "body-parser": "^1.12.3",
    "cookie-parser": "^1.3.4",
    "express": "^4.11.1",
    "express-session": "^1.11.1",
    "flux": "^2.0.1",
    "less": "^2.4.0",
    "lodash": "^3.7.0",
    "marty": "^0.9.11",
    "marty-express": "^0.2.0",
    "moment": "^2.10.2",
    "object-assign": "^2.0.0",
    "passport": "^0.2.1",
    "passport-openidconnect": "0.0.1",
    "react": "^0.13.0",
    "react-bootstrap": "^0.21.2",
    "react-bootstrap-datetimepicker": "0.0.14",
    "react-router": "^0.13.0",
    "superagent": "^1.2.0"
  },
  "browserify": {
    "transform": [
      [
        "babelify"
      ]
    ]
  },
  "jest": {
    "scriptPreprocessor": "<rootDir>/node_modules/babel-jest",
    "unmockedModulePathPatterns": [
      "node_modules/react"
    ]
  },
  "engines": {
    "node": "0.12.0"
  },
  "apps" : [{
    "script"      : "app/server.js",
    "name"        : "react-travel",
    "next_gen_js" : true,
    "autorestart" : false,
    "watch"       : false
  }]
}
